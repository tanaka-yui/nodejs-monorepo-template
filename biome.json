{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
	  "enabled": true,
	  "clientKind": "git"
	},
	"files": {
	  "ignoreUnknown": false,
	  "ignore": [
		"**/gen/**"
	  ]
	},
	"formatter": {
	  "enabled": true,
	  "formatWithErrors": false,
	  "attributePosition": "auto",
	  "indentStyle": "space",
	  "indentWidth": 2,
	  "lineWidth": 120,
	  "lineEnding": "lf"
	},
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true,
		"correctness": {
		  "noUndeclaredVariables": "error",
		  "noUnusedVariables": "error",
		  "noUnusedImports": "error"
		}
	  }
	},
	"javascript": {
	  "formatter": {
		"quoteStyle": "single",
		"semicolons": "asNeeded"
	  }
	}
  }
  