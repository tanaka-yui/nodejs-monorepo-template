{
  "name": "@nodejs-mono-repo/frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "dev": "vite",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "openapi:generate": "openapi-generator-cli generate -i http://localhost:8889/docs/json -g typescript-fetch -o ./src/gen/api",
    "preview": "vite preview"
  },
  "dependencies": {
    "@date-fns/tz": "1.2.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@hookform/resolvers": "3.10.0",
    "@mui/base": "5.0.0-beta.69",
    "@mui/icons-material": "6.4.1",
    "@mui/material": "6.4.1",
    "@mui/system": "6.4.1",
    "@tanstack/react-query": "5.64.2",
    "i18next": "24.2.2",
    "i18next-browser-languagedetector": "8.0.2",
    "jotai": "2.11.1",
    "jotai-effect": "1.1.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "5.0.0",
    "react-hook-form": "7.54.2",
    "react-i18next": "15.4.0",
    "react-icons": "5.4.0",
    "yup": "1.6.1"
  },
  "devDependencies": {
    "@nodejs-mono-repo/typescript-config": "workspace:*",
    "@nodejs-mono-repo/utils": "workspace:*",
    "@openapitools/openapi-generator-cli": "^2.16.2",
    "@tanstack/react-query-devtools": "5.64.2",
    "@tanstack/react-router": "1.97.20",
    "@tanstack/router-devtools": "1.97.20",
    "@tanstack/router-plugin": "1.97.20",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react-swc": "3.7.2",
    "rollup": "4.32.0",
    "rollup-plugin-visualizer": "5.14.0",
    "typescript": "5.7.3",
    "vite": "6.0.11",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4"
